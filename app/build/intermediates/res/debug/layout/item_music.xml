<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="92dp"
                android:minHeight="92dp"
    android:gravity="center_vertical"
    android:orientation="vertical"
    android:padding="16dp">
    <TextView
        android:id="@+id/title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:text="TextView"
        android:layout_alignParentLeft="true"
        android:singleLine="true"
        android:ellipsize="middle"
        android:layout_toLeftOf="@+id/isSelect"
        android:textColor="@color/text_colot_1"
        android:textSize="16sp" />


    <TextView
        android:layout_alignParentLeft="true"
        android:singleLine="true"
        android:ellipsize="middle"
        android:layout_toLeftOf="@+id/isSelect"
        android:textSize="14sp"
        android:layout_below="@+id/title"
        android:id="@+id/size"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        tools:text="TextView"
        android:textColor="@color/text_colot_2" />

    <com.zjk.wifiproject.view.TouchCheckBox
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:id="@+id/isSelect"
        android:layout_width="48dp"
        android:padding="10dp"
        android:layout_height="48dp"
        android:checked="false"/>

</RelativeLayout><!-- From: file:/E:/bysj/WifiProject-AS/WifiProject/app/src/main/res/layout/item_music.xml -->